{
  "name": "iframe",
  "private": true,
  "version": "0.0.79",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && pnpm build:init && pnpm merge-files",
    "build:init": "tsdown",
    "merge-files": "cat ./zoid/zoid.js ./zoid/zaptime-init.js > ./dist/zaptime.js",
    "preview": "vite preview",
    "deploy": "wrangler deploy --name iframe --compatibility-date 2026-02-17 --assets dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@zaptime/core": "workspace:*",
    "@zaptime/vue3": "workspace:*",
    "vue": "^3.4.19"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zaptime/zaptime-monorepo"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^3.0.5",
    "autoprefixer": "^10.4.17",
    "happy-dom": "^17.4.4",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsdown": "^0.9.4",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "vitest": "^3.0.5",
    "vue-tsc": "^2.2.8"
  }
}